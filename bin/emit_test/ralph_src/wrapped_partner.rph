alias Endpoint SideA as 'ralph_emitted.BasicPartnerJava.SideA';


Endpoint SingleSidedHolder
{
    Endpoint SideA endpt = null;

    set_endpoint(Endpoint SideA to_set_to)
    {
        endpt = to_set_to;
    }

    issue_call()
    {
        atomically {
            endpt.increment_other_side_number(1.0);
        }
    }
}
