alias Service BasicSetterGetter as 'BasicRalphJava.SetterGetter';


Endpoint EndpointUser
{
    Service BasicSetterGetter local_end = null;

    set_endpoint(Service BasicSetterGetter to_set_to)
    {
        local_end = to_set_to;
    }

    get_number() returns Number
    {
        Number to_return = local_end.get_number();
        return to_return;
    }

    set_number(Number new_number)
    {
        local_end.set_number(new_number);
    }
    
    get_text() returns Text
    {
        return local_end.get_text();
    }

    set_text(Text new_text)
    {
        local_end.set_text(new_text);
    }

    get_tf() returns TrueFalse
    {
        return local_end.get_tf();
    }

    set_tf(TrueFalse new_tf)
    {
        local_end.set_tf(new_tf);
    }
    
}
